{
  "manifest_version": 2,
  "name": "ImageHint",
  "version": "0.0.1",
  "author":"Avery Hiebert",
  "homepage_url":"https://github.com/averyhiebert/ImageHint",
  "description": "Provides visual hints for language learners.",
